{
  "name": "cursor-price-tracking",
  "displayName": "Cursor Usage & Cost Tracker",
  "description": "Monitor your Cursor AI usage, token consumption, and costs in real-time with detailed breakdowns by model and session",
  "version": "0.0.1",
  "publisher": "Ittipong",
  "icon": "logo.png",
  "keywords": [
    "cursor",
    "ai",
    "usage",
    "cost",
    "tracking",
    "tokens",
    "pricing",
    "claude",
    "gpt",
    "monitoring"
  ],
  "homepage": "https://github.com/Ittipong/cursor-price-tracking",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ittipong/cursor-price-tracking.git"
  },
  "bugs": {
    "url": "https://github.com/Ittipong/cursor-price-tracking/issues"
  },
  "license": "MIT",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other",
    "Debuggers",
    "Data Science"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:cursorPriceTracking.refresh",
    "onCommand:cursorPriceTracking.configure",
    "onView:cursorPrices"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "capabilities": {
      "virtualWorkspaces": true,
      "untrustedWorkspaces": {
        "supported": true
      }
    },
    "commands": [
      {
        "command": "cursorPriceTracking.refresh",
        "title": "Refresh Usage Data",
        "icon": "$(refresh)",
        "category": "Cursor Price Tracking"
      },
      {
        "command": "cursorPriceTracking.configure",
        "title": "Configure Session Token",
        "category": "Cursor Price Tracking"
      },
      {
        "command": "cursorPriceTracking.reset",
        "title": "Reset Configuration",
        "category": "Cursor Price Tracking"
      }
    ],
    "configuration": {
      "title": "Cursor Price Tracking",
      "properties": {
        "cursorPriceTracking.sessionToken": {
          "type": "string",
          "default": "",
          "description": "Manual fallback: Cursor session token for API authentication. The extension auto-discovers this from Cursor's local database, so you typically don't need to set this.",
          "markdownDescription": "**Usually not needed!** The extension automatically reads your session token from Cursor's local database.\n\nOnly set this manually if auto-discovery fails:\n1. Go to [cursor.com](https://cursor.com) and log in\n2. Open browser DevTools (F12)\n3. Go to Application/Storage > Cookies\n4. Copy the `WorkosCursorSessionToken` value\n5. Paste it here or use the 'Configure Session Token' command"
        },
        "cursorPriceTracking.refreshInterval": {
          "type": "number",
          "default": 30,
          "minimum": 5,
          "description": "Auto-refresh interval in seconds. Set to 0 to disable auto-refresh."
        }
      }
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "cursorPricePanel",
          "title": "Cursor Price Tracking",
          "icon": "$(dollar)"
        }
      ]
    },
    "views": {
      "cursorPricePanel": [
        {
          "id": "cursorPrices",
          "name": "Cursor Usage",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "cursorPriceTracking.refresh",
          "when": "view == cursorPrices",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "install": "vsce package && code --install-extension cursor-price-tracking-0.0.1.vsix"
  },
  "dependencies": {
    "sql.js": "^1.11.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}